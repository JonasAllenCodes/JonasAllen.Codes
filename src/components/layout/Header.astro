---
import Nav from "./Nav.astro";
import Logo from "./Logo.astro";

interface Props {
    title?: string;
}

const { title } = Astro.props;
---

<header class="header">
    <Logo title={title} />
    <Nav />
</header>

<style>
    .header {
        width: 100%;
        max-width: var(--page-width);
        margin-inline: auto;
    }
</style>